package com.loaine.cursojava.aula13;

public class OperadoresAritimeticos {

	public static void main(String[] args) {
		int resultado=1+2;
		System.out.println(resultado);
		
		resultado=resultado-1;
		System.out.println(resultado);
		
		resultado=resultado*3;
		System.out.println(resultado);
		
		

	}

}
