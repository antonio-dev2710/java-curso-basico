package com.loaine.cursojava.lista08;

public class TesteLampada {

	public static void main(String[] args) {
		
		Lampada led = new Lampada();
		
		//passando valor pra o metodo set
				led.setTipo("Arco");
				//pegando o valor setado
		System.out.println("O tipo é :"+led.getTipo());
		
		led.setPreco(8.39);
		System.out.println("O preço é :"+led.getPreco());
		led.setVoltagem(1500);
		System.out.println("O preço é :"+led.getVoltagem());
		
		
		 
		 System.out.println("Vc encerrou o metodo");
		 
		 

	}

}
