package com.loaine.cursojava.lista10;

public class Fibo {

	public static  int nA=0;
	 private static  int nB=1;
	 private static  int n;
	 private static int i=0;
	//metodo
	public  static  int  fibonacci(int total ) {
		
		if(i==10) {
			return n;
		}
		n= nA+nB;
		nA=nB;
		nB=n;
		i++;
		System.out.println( );
		
		return fibonacci(total=n);
	}

}
