package com.loaine.cursojava.lista03;

import java.util.Scanner;

public class Questao26 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//verificar quantas notas tem em cima do valor
		Scanner scan=new Scanner(System.in);
		
		String[][] table = new String[4][];
        table[0] = new String[] { "Especificaçao", "Código", "Preço" };
        table[1] = new String[] { "cachorro quente","bauro simples","bauro com ovo","hamburguer","x burguer","Refri" };
        table[2] = new String[] { "Carrots", "Beans", "Cabbage" };
        table[3] = new String[] { "Candy", "Cake", "Bread" };

        for (String[] row : table) {
       
            System.out.format(" %15s %15s %n", row);
            
            
        }
				
				
		

		
 }
}
