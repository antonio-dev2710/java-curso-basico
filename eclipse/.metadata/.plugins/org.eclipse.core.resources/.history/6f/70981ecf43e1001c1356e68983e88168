package com.loaine.cursojava.lista03;


public class Questao12 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int numero = 1;
		
		int max = 10;
		
		System.out.println(numero);
		for(int i=0;i<max;i++) {
			
			numero=numero+i;
			//int total=
			System.out.println(numero);
		}
	}

}
