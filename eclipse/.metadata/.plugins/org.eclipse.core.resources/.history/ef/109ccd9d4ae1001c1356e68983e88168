package com.loaine.cursojava.lista03;


public class Questao12 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int numero = 0;
		
		int max = 10;
		
		int total=0;
		int resp=0;
		for(int i=0;i<max;i++) {
			numero++;
			total=numero+total;
			numero--;
			resp=total+numero;
			System.out.println(resp);
			
			

			
		}
	}

}
