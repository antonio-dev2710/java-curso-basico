package com.loaine.cursojava.lista10;

public class ConversaoDeUndArea {
	
	public static double converterMeP(double num) {
		num*=10.76;
		return num;
	}
	
	public static double converterPeM(double num) {
		
		return num/10.76;
	}
	
	public static double converterPeC(double num) {
		
		return num*927;
	}
	public static double converterCentP(double num) {
	
	return num/927;
}

	public static double converterMiA(double num) {
	
	return num*640;
}
	public static double converterAcM(double num) {

return num/640;
}
	
	

}
