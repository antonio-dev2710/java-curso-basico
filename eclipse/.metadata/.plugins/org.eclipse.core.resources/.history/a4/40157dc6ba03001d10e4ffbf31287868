package com.loaine.cursojava.lista08;

public class questoes {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 3;

		int b = 5;

		int c = a * b + 5;
		

		a = a + 8 / 2;
		
		b = 5 * b * 5;
		
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);

	}

}
